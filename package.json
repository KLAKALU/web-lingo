{
  "name": "web-lingo",
  "displayName": "Web lingo",
  "version": "0.0.1",
  "description": "A browser extension that provides word definitions and translations for words on web pages.",
  "author": "ETS",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "test": "plasmo test"
  },
  "dependencies": {
    "@parcel/optimizer-htmlnano": "2.9.3",
    "@plasmohq/storage": "^1.9.0",
    "axios": "^1.6.2",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.48.2",
    "svgo": "^3.0.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.5",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "scripting",
      "action",
      "storage",
      "tabs",
      "contextMenus",
      "contextMenus.onClicked.addListener"
    ]
  }
}
